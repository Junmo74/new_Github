Function GetRoomCode(ToControl As Object) As String
  If TypeName(ToControl) = "ListBox" Then
    s = Mid(ToControl.name, 2, 5)
    If (Left(s, 1) = "6") Then
      GetRoomCode = s
    End If
  End If
  
End Function
