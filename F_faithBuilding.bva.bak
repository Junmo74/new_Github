' 해당방에 배정/소스 리스트박스는 어차피 고정이므로 하드코딩 / 메인 리스트 박스의 값을 얻어온 숙소코드로 적용
Private Sub RoomApplyProcedure(cCode As String, pToControl As Object)
  If cCode <> "" Then
    Dim intc As Integer
  
    Set cDB = CurrentDb()
    sSQL = "Update T_수양회접수_기본 set 숙소코드 = " + cCode + " Where 접수아이디 in ("
    
    intc = L_Main.ItemsSelected.Count
    For k = 0 To intc - 1
      If k = (intc - 1) Then
        sSQL = sSQL + L_Main.itemData(L_Main.ItemsSelected(k)) + ")"
      Else
       sSQL = sSQL + L_Main.itemData(L_Main.ItemsSelected(k)) + ","
      End If
    Next

    cDB.Execute sSQL
    cDB.Close
    
    SelectedItemsInit L_Main

    Me.L_Main.Requery '배정 인원이 감소했기 때문에 리쿼리

    pToControl.Requery '인원이 숙소에 추가 되었기 때문에 리쿼리
   '=================================================== 해당 숙소 인원 수 표기
    
    PersonInCurrentRoom Me("B" + Mid(pToControl.name, 2, 5)), CStr(pToControl.ListCount)
  End If
End Sub

'== 각 숙소에 배정된 인원 갱신
Public Sub PersonInCurrentRoom(cObj As Object, sCnt As String)
  ' 숙소표에서 호실 찾아와서 붙이기
  Dim rstGetCode As Recordset
  
  s = "SELECT 방호수 FROM T_숙소목록표 WHERE 숙소코드 LIKE '" & Mid(cObj.name, 2, 5) & "'"
  Set cDB = CurrentDb()
  Set rstGetCode = cDB.OpenRecordset(s, dbOpenForwardOnly)
  
  cObj.Caption = rstGetCode!방호수 & " : " & sCnt & " 명"
  
  rstGetCode.Close
  cDB.Close
End Sub

'방 빼는 프로시져==================================================================================================
Private Sub DeletePersonInRoom(sqlPersonDeleteInRoom As String, pToControl As Object)
  intc = pToControl.ItemsSelected.Count
  Set cDB = CurrentDb()

  sSQL = sqlPersonDeleteInRoom
    
  For k = 0 To intc - 1
    If k = (intc - 1) Then '한 방에서 여러명 선택해서 뺄때 루프 돌아야 함
      sSQL = sSQL + pToControl.itemData(pToControl.ItemsSelected(k)) + ")"
    Else
      sSQL = sSQL + pToControl.itemData(pToControl.ItemsSelected(k)) + ","
    End If
  Next
    
  If sSQL = "" Then
    Exit Sub
  End If
    
  cDB.Execute sSQL
  cDB.Close
    
  pToControl.Requery ' 현재 선택된 방 리쿼리

  PersonInCurrentRoom Me("B" + Mid(pToControl.name, 2, 5)), CStr(pToControl.ListCount)
End Sub

Private Sub btn_숙소초기화_Click()
  Dim ctl As Control
  Dim s As String
  Dim intc As Integer
  
  For Each ctl In Me.Controls
  s = Mid(ctl.name, 1, 3)
  
    If TypeName(ctl) = "ListBox" And (s = "L65") Then
      ctl.RowSource = sqlRoomRecords + GetRoomCode(ctl) + sqlRoomRecordsSort
      ctl.Requery

      ctl.ColumnCount = "6"
      ctl.ColumnWidths = "0cm;0cm;1.4cm;1.3cm;0cm;0.6cm;"

      PersonInCurrentRoom Me("B" + Mid(ctl.name, 2, 5)), CStr(ctl.ListCount)
      
      btn_숙소초기화.Tag = "1" ' 배정 현황을 최초 한번은 실행후 배정가능
    End If
  Next
End Sub

Private Sub SetBorder(ctl As Access.Control, ApplyBorder As TypeMarkBorder)
    With ApplyBorder
        ctl.BorderColor = .BorderColor
        ctl.BorderStyle = .BorderStyle
        ctl.BorderWidth = .BorderWidth
    End With
End Sub

Private Sub SelectedItemsInit(obj As Object) '리스트 박스에 선택항목 미선택으로 초기화
  Dim intCurrentRow As Integer
  For intCurrentRow = 0 To obj.ListCount - 1
    If obj.Selected(intCurrentRow) Then
      obj.Selected(intCurrentRow) = False
    End If
  Next intCurrentRow
End Sub


' 현재 방 컨트롤 설정/ 이 프로시져는 반드시 올바로 발생하게 되어 있음/인자의 널 상태 오류 대처할 필요 없음
Public Sub BackupCurrentRoom(oCur As Object) '==========================================================================
' 1. 처음 현재 컨트롤 선택의 경우
'    가. 현재 컨트롤 백업
'    나. 현재 컨트롤 보드 속성 변경
' 2. 이전 컨트롤 에서 현재 선택의 경우
'    가. 이전 컨트롤 초기화
'    나. 이전 컨트롤 보드 원래상태로 복원

  If Not CaptureCurrentRoom Is Nothing Then
    ' 1. 이전에 백업된 숙소가 있으면 선택 아이템레코드 모두 초기화
    SelectedItemsInit CaptureCurrentRoom
    ' 2. 백업 숙소의 보드 속성 원상태 복구
    SetBorder CaptureCurrentRoom, OriginalRoomBorder
  End If

    ' 현재 선택된 리스트박스 백업 저장
  Set CaptureCurrentRoom = oCur
  With oCur
      .BorderColor = vbGreen
      .BorderStyle = 4 ' Dots
      .BorderWidth = 3
  End With
  
End Sub


