Private Sub cmdSaveData_baedanibuilding_Click()
  Dim fd As Object, xmlPath As String, strXml As String, strConn As String
  Dim sNow As String
  sNow = Format(Now(), "yyyymmdd_hhnnss")
  strXml = "믿음관배정현황_" & sNow & ".xlsx"

  Set fd = Application.FileDialog(2)
  With fd
  
    .Title = ".xlsx 파일로 저장"
    .ButtonName = "저장"
    .InitialFileName = CurrentProject.Path & "\" & strXml
    If .Show = True Then
        xmlPath = .SelectedItems(1)
        
        ' 테이블 명 기입란에 쿼리도 가능하다.
        If FileExists(xmlPath) Then
          SetAttr xmlPath, vbNormal
          Kill xmlPath
        End If
        '=========================================================================================================================
        DoCmd.TransferSpreadsheet acExport, acSpreadsheetTypeExcel12Xml, "Q_믿음관배정현황", xmlPath, -1
        '=========================================================================================================================
    Else
      MsgBox "파일 대화 상자에서 취소를 선택했습니다."
      Exit Sub
    End If
      MsgBox "작업중이던 수양회 접수현황을 저장하였습니다.", vbOKOnly
  End With
End Sub

