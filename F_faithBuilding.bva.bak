Private Sub btn_배정초기화_Click()

  Const cstrPrompt As String = "모든 숙소에 배정된 인원을 배정 취소 하시겠습니까..?"
  If MsgBox(cstrPrompt, vbQuestion + vbYesNo) = vbYes Then
    If btn_숙소초기화.Tag = "1" Then '테그를 변수처럼 활용하기/최초 배정현황 가져오기가 이루어 졌는지..?
      Set cDB = CurrentDb()
      cDB.Execute sqlClearRoom
      cDB.Close
    
      btn_숙소초기화.Tag = ""
   
      SelectedItemsInit L_Main
      Me.L_Main.Requery '배정 인원이 감소했기 때문에 리쿼리

      Dim ctl As Control
      Dim s As String

      For Each ctl In Me.Controls
      s = Mid(ctl.name, 1, 3)

        If TypeName(ctl) = "ListBox" And (s = "L65") Then
          ctl.RowSource = ""
          ctl.Requery
          
          PersonInCurrentRoom Me("B" + Mid(ctl.name, 2, 5)), CStr(0)
        End If
      Next
    End If
  End If
End Sub

