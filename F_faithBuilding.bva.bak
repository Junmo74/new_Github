Private Sub L65041_MouseDown(Button As Integer, Shift As Integer, x As Single, y As Single)
On Error GoTo Err_L65041_MouseDown

  If Me.L65041.ListCount > 0 Then
    StartDrag Me
  End If

End_L65041_MouseDown:
    Exit Sub

Err_L65041_MouseDown:
    MsgBox err.Description & " (" & err.Number & ") in " & IIf(err.Source = Me.name, Me.name & ".L65041_MouseDown", err.Source) & vbOKOnly & vbCritical, "에러 메세지"
    Resume End_L65041_MouseDown

End Sub

